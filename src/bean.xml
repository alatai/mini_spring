<?xml version="1.0" encoding="UTF-8" ?>
<beans>
    <!-- basebaseservice -->
    <bean id="basebaseservice"
          class="com.alatai.test.service.impl.BaseBaseService">
        <property type="com.alatai.test.service.impl.AServiceImpl" name="as"
                  ref="aservice"/>
    </bean>
    <!-- aservice -->
    <bean id="aservice" class="com.alatai.test.service.impl.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="com.alatai.test.service.impl.BaseService" name="ref1"
                  ref="baseservice"/>
    </bean>
    <!-- baseservice -->
    <bean id="baseservice" class="com.alatai.test.service.impl.BaseService">
        <property type="com.alatai.test.service.impl.BaseBaseService"
                  name="bbs" ref="basebaseservice"/>
    </bean>
</beans>